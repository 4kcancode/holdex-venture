{
	"name": "holdex-venture-studio",
	"version": "0.0.1",
	"scripts": {
		"pre-dev": "node pull-env.js",
		"dev": "npm run pre-dev && npm run generate && vite dev",
		"build": "npm run generate && vite build",
		"preview": "npm run generate  && vite preview",
		"generate": "graphql-codegen --config codegen.yml -r dotenv/config",
		"postinstall": "svelte-kit sync && sh ./apollo-client-patch.sh",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npx eslint --ext js,ts,cjs,svelte --ignore-path .eslintignore --fix .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.2.2",
		"@graphql-codegen/typescript-operations": "^3.0.2",
		"@steeze-ui/heroicons": "^2.2.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/lodash-es": "^4.17.7",
		"@types/numeral": "^2.0.2",
		"@types/rimraf": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prefer-arrow-functions": "^3.1.4",
		"eslint-plugin-svelte3": "^4.0.0",
		"form-data": "^4.0.0",
		"fs-extra": "^11.1.1",
		"postcss": "^8.4.21",
		"postcss-import": "^15.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"pug": "^3.0.2",
		"sass": "^1.60.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0"
	},
	"dependencies": {
		"@apollo/client": "^3.7.10",
		"@github/details-dialog-element": "^3.1.3",
		"apexcharts": "^3.37.3",
		"dotenv": "^16.0.3",
		"editorjs-html": "^3.4.2",
		"graphql": "^16.6.0",
		"insane": "^2.6.2",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.4",
		"node-fetch": "^3.3.1",
		"numeral": "^2.0.6",
		"rimraf": "^4.4.1",
		"rollbar": "^2.26.1"
	},
	"type": "module"
}
