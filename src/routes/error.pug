include ./(pages)/common-mixins

mixin went-wrong
  img(
    src="data:image/png;base64,"
    alt="Not found"
  )
  p(class="text-paragraph-l max-w-[400px] w-full text-center mb-4") Something went wrong!.
  +if('isDev')
    | {JSON.stringify($page.error || {})}
  +primary-button('Reload Page')(on:click!="{() => window.location.reload()}")

mixin not-found
  img(
    src="data:image/png;base64,"
    alt="Not found"
  )
  p(class="text-paragraph-l max-w-[400px] w-full text-center mb-4") The page you’re looking for could have been deleted, moved or never existed.
  +primary-button('Take me to the base 🤦‍♂️')(on:click!="{() => goto('/')}")

.app-container
  main(class="flex flex-col items-center justify-center xs:my-10")
    +if("$page.error.message.includes('Not found')")
      +not-found
      +elseif("$page.error.message.includes('not_found')")
        +not-found
        +else
          +went-wrong
